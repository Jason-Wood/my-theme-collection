Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter: lcddefault
Xft.autohint: true

URxvt.intensityStyles:  false
!URxvt.font: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-1
!URxvt.font: -misc-fixed-bold-r-normal--15-140-75-75-c-90-iso8859-1
URxvt.font:             xft:Iosevka:bold:size=9:antialias=true:hinting=true, \
			xft:EmojiOne Color:antialias=false, \
			xft:Noto Mono:size=10:antialias=true:hinting=true,\
			xft:Blobmoji, \
			xft:Noto Emoji:size=13
!            xft:Kochi Gothic:antialias=false,\
!            xft:Baekmuk Gulim
URxvt.boldFont:         xft:Iosevka:bold:size=9:antialias=true:hinting=true, \
			xft:EmojiOne Color:antialias=false, \
			xft:Noto Mono:size=10:antialias=true:hinting=true,\
			xft:Blobmoji,\
			xft:Noto Emoji:size=13
!            xft:Kochi Gothic:antialias=false,\
!            xft:Baekmuk Gulim
URxvt.saveLines:        8192

URxvt*scrollTtyOutput:    false
URxvt*scrollWithBuffer:   true
URxvt*scrollTtyKeypress:  true
URxvt.scrollBar:          false
URxvt.internalBorder:     30
URxvt.cursorBlink:        false
URxvt.cursorUnderline:    true
URxvt*skipBuiltinGlyphs:    true

URxvt.perl-ext-common: font-size,keyboards-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate

URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show
!URxvt.letterSpace: -1
!URxvt.lineSpace: -1
URxvt.geometry:	80x10

